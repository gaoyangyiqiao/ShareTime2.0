                        android:background="#ff298cff"

                        android:clickable="true" />
                    <ImageButton
                        android:layout_width="63dp"
                        android:layout_height="48dp"
                        android:id="@+id/ib_match_detail_right_5"
                        android:background="#ff298cff"

                        android:clickable="true" />
                    <ImageButton
                        android:layout_width="63dp"
                        android:layout_height="48dp"
                        android:id="@+id/ib_match_detail_right_6"
                        android:background="#ff298cff"

                        android:clickable="true" />
                    <ImageButton
                        android:layout_width="63dp"
                        android:layout_height="48dp"
                        android:id="@+id/ib_match_detail_right_7"
                        android:background="#ff298cff"

                        android:clickable="true" />
                    <ImageButton
                        android:layout_width="63dp"
                        android:layout_height="48dp"
                        android:id="@+id/ib_match_detail_right_8"
                        android:background="#ff298cff"

                        android:clickable="true" />
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </FrameLayout>
</LinearLayout>
