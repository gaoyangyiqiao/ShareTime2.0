                        />
                    <TextView
                        android:layout_width="63dp"
                        android:layout_height="48dp"
                        android:id="@+id/tv_match_detail_36"
                        />
                    <TextView
                        android:layout_width="63dp"
                        android:layout_height="48dp"
                        android:id="@+id/tv_match_detail_37"
                        />
                    <TextView
                        android:layout_width="63dp"
                        android:layout_height="48dp"
                        android:id="@+id/tv_match_detail_38"
                        />
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <ImageButton
                        android:layout_width="63dp"
                        android:layout_height="48dp"
                        android:id="@+id/ib_match_detail_right_1"
                        android:background="#ff298cff"

                        android:clickable="true" />
                    <ImageButton
                        android:layout_width="63dp"
                        android:layout_height="48dp"
                        android:id="@+id/ib_match_detail_right_2"
                        android:background="#ff298cff"

                        android:clickable="true" />
                    <ImageButton
                        android:layout_width="63dp"
                        android:layout_height="48dp"
                        android:id="@+id/ib_match_detail_right_3"
                        android:background="#ff298cff"

                        android:clickable="true" />
                    <ImageButton
                        android:layout_width="63dp"
                        android:layout_height="48dp"
